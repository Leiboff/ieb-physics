    <mtr>
      <mtd>
        <mi>p</mi>
      </mtd>
      <mtd>
        <mo>=</mo>
        <mo stretchy="false">(</mo>
        <mn>1000</mn>
        <mo stretchy="false">)</mo>
        <mo stretchy="false">(</mo>
        <mn>8</mn>
        <mo stretchy="false">)</mo>
      </mtd>
    </mtr>
    <mtr>
      <mtd>
        <mi>p</mi>
      </mtd>
      <mtd>
        <mo>=</mo>
        <mn>8000</mn>
        <mtext>&nbsp;</mtext>
        <msup>
          <mtext>kg.m.s</mtext>
          <mrow class="MJX-TeXAtom-ORD">
            <mo>−<!-- − --></mo>
            <mn>1</mn>
          </mrow>
        </msup>
      </mtd>
    </mtr>
  </mtable>
<annotation encoding="math/tex">\begin{align}p &amp;= mv \\
p &amp;= (1000)(8) \\
p &amp;= 8000 \ \text{kg.m.s}^{-1}
\end{align}</annotation></semantics></math></p><p></p></div><div class="solution">
