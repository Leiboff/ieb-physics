                \vec{v}_{f}&amp; = \vec{v}_{i}+\vec{a}t \\
                \Delta \vec{x}&amp; = \frac{\left(\vec{v}_{i}+\vec{v}_{f}\right)}{2}t \\
                \Delta \vec{x}&amp; = \vec{v}_{i}t+\frac{1}{2}\vec{a}{t}^{2} \\
                \vec{v}_{f}^{2}&amp; = \vec{v}_{i}^{2}+2\vec{a}\Delta\vec{ x }
